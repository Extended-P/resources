<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/op_apps"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <Preference android:title="@string/app_notifications_title" android:key="oneplus_applications_notifcaions_manager_settings" android:fragment="com.android.settings.applications.NotificationApps">
        <extra android:name="classname" android:value="com.android.settings.Settings$NotificationAppListActivity" />
    </Preference>
    <com.oneplus.settings.ui.OPPreferenceDivider android:key="preference_divider_line" />
    <PreferenceCategory android:title="@string/applications_settings" android:key="applications_settings_key">
        <Preference android:title="@string/oneplus_app_list" android:key="oneplus_applications_manager_settings" android:fragment="com.android.settings.applications.ManageApplications" />
        <Preference android:title="@string/default_apps_title" android:key="onepluse_default_applications_setting" android:fragment="com.oneplus.settings.OPDefaultAppSettings" />
        <Preference android:title="@string/oneplus_multi_app" android:key="onepluse_multi_applications_setting" />
    </PreferenceCategory>
    <com.oneplus.settings.ui.OPPreferenceDivider android:key="preference_divider_line" />
    <PreferenceCategory android:title="@string/permissions_label" android:key="applications_permissions_settings_key">
        <Preference android:title="@string/app_permissions" android:key="oneplus_applications_permission_settings" />
        <Preference android:title="@string/oneplus_app_network_settings" android:key="oneplus_applications_network_settings" />
        <Preference android:title="@string/special_access" android:key="onepluse_special_access" android:fragment="com.android.settings.applications.SpecialAccessSettings" />
    </PreferenceCategory>
</PreferenceScreen>