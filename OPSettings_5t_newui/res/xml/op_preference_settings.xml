<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:layout_width="wrap_content" android:layout_height="wrap_content"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
    <com.android.settings.fuelgauge.WallOfTextPreference android:selectable="false" android:key="preference_settings_summary" android:summary="@string/alarms_summary" settings:allowDividerAbove="true" />
    <PreferenceCategory android:title="@string/oneplus_preference_settings_manual_open" android:key="preference_settings_manual_open">
        <SwitchPreference android:title="@string/oneplus_preference_settings_turn_on" android:key="preference_settings_turn_on" />
    </PreferenceCategory>
    <com.oneplus.settings.ui.OPPreferenceDivider android:key="preference_divider_line" />
    <PreferenceCategory android:title="@string/oneplus_preference_settings_auto_activate" android:key="preference_settings_auto_activate">
        <SwitchPreference android:title="@string/oneplus_preference_settings_custom_time" android:key="preference_settings_custom_time" />
        <Preference android:title="@string/oneplus_preference_settings_turn_on_time" android:key="preference_settings_turn_on_time" />
        <Preference android:title="@string/oneplus_preference_settings_turn_off_time" android:key="preference_settings_turn_off_time" />
    </PreferenceCategory>
    <com.oneplus.settings.ui.OPPreferenceDivider android:key="preference_divider_line" />
    <PreferenceCategory android:title="@string/oneplus_preference_settings_behavior" android:key="preference_settings_behavior">
        <Preference android:title="@string/oneplus_preference_settings" android:key="preference_settings" android:fragment="com.android.settings.notification.ZenModePrioritySettings" />
    </PreferenceCategory>
</PreferenceScreen>